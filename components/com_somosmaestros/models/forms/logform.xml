<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset>

              <field name="id" type="text" default="0" label="COM_SOMOSMAESTROS_FORM_LBL_LOG_ID"
            readonly="true" class="readonly"
            description="JGLOBAL_FIELD_ID_DESC" /> 

       <field name="created_by" type="createdby" default="" 
            label="COM_SOMOSMAESTROS_FORM_LBL_LOG_CREATED_BY"
            description="COM_SOMOSMAESTROS_FORM_DESC_LOG_CREATED_BY"  /> 

       <field name="date" type="text"
                
            label="COM_SOMOSMAESTROS_FORM_LBL_LOG_DATE"
            description="COM_SOMOSMAESTROS_FORM_DESC_LOG_DATE" 
                
                
                 required="true"  /> 

       <field name="time" type="text"
                
            label="COM_SOMOSMAESTROS_FORM_LBL_LOG_TIME"
            description="COM_SOMOSMAESTROS_FORM_DESC_LOG_TIME" 
                
                
                 required="true"  /> 

       <field name="vista" type="text"
                
            label="COM_SOMOSMAESTROS_FORM_LBL_LOG_VISTA"
            description="COM_SOMOSMAESTROS_FORM_DESC_LOG_VISTA" 
                
                
                 required="true"  /> 

       <field name="cedula" type="text"
                
            label="COM_SOMOSMAESTROS_FORM_LBL_LOG_CEDULA"
            description="COM_SOMOSMAESTROS_FORM_DESC_LOG_CEDULA" 
                
                
                 required="true"  /> 

         <field
        name="state"
        type="list"
        label="JSTATUS"
        description="JFIELD_PUBLISHED_DESC"
        class="inputbox"
        size="1"
        default="1">
        <option value="1">JPUBLISHED</option>
        <option value="0">JUNPUBLISHED</option>
        <option value="2">JARCHIVED</option>
        <option value="-2">JTRASHED</option>
    </field> 

                        <field name="checked_out" type="hidden" filter="unset" />
        <field name="checked_out_time" type="hidden" filter="unset" />

	</fieldset>

</form>
